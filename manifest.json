{
  "manifest_version": 3,
  "name": "Chord Gosu",
  "version": "1.0",
  "description": "Interactive Guitar Chord Trainer & Quiz",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "permissions": [
    "storage"
  ]
}